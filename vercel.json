{
  "version": 2,
  "routes": [
    { "src": "/health", "destination": "/api/index.ts" },
    { "src": "/app/v1/(.*)", "destination": "/api/index.ts" },
    { "src": "/(.*)", "destination": "/api/index.ts" }
  ],
  "functions": {
    "api/index.ts": {
      "memory": 1024,
      "maxDuration": 10
    }
  }}